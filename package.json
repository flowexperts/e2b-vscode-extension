{
  "name": "e2b-sandbox-explorer",
  "displayName": "E2B Sandbox Explorer",
  "description": "Connect to E2B sandboxes, browse files, and run commands directly from VS Code",
  "version": "0.1.5",
  "publisher": "bhavaniravi",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/flowexperts/e2b-vscode-extension"
  },
  "bugs": {
    "url": "https://github.com/flowexperts/e2b-vscode-extension/issues"
  },
  "homepage": "https://e2b.dev",
  "keywords": [
    "e2b",
    "sandbox",
    "remote",
    "cloud",
    "development",
    "terminal",
    "file explorer",
    "remote development"
  ],
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:e2b",
    "onView:e2bSandboxes"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "e2b-explorer",
          "title": "E2B Sandboxes",
          "icon": "./icon.svg"
        }
      ]
    },
    "views": {
      "e2b-explorer": [
        {
          "id": "e2bSandboxes",
          "name": "Sandboxes",
          "icon": "$(cloud)",
          "contextualTitle": "E2B Sandboxes"
        },
        {
          "id": "e2bFiles",
          "name": "Files",
          "when": "e2b.connected"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "e2bSandboxes",
        "contents": "No API key configured.\n[Set API Key](command:e2b.setApiKey)",
        "when": "!e2b.hasApiKey"
      },
      {
        "view": "e2bFiles",
        "contents": "No sandbox connected.\nSelect a sandbox from the list above.",
        "when": "!e2b.connected"
      }
    ],
    "commands": [
      {
        "command": "e2b.setApiKey",
        "title": "E2B: Set API Key"
      },
      {
        "command": "e2b.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "e2b.connect",
        "title": "Connect",
        "icon": "$(plug)"
      },
      {
        "command": "e2b.disconnect",
        "title": "E2B: Disconnect",
        "icon": "$(debug-disconnect)"
      },
      {
        "command": "e2b.openTerminal",
        "title": "E2B: Open Terminal",
        "icon": "$(terminal)"
      },
      {
        "command": "e2b.openFile",
        "title": "Open File"
      },
      {
        "command": "e2b.newFile",
        "title": "New File",
        "icon": "$(new-file)"
      },
      {
        "command": "e2b.newFolder",
        "title": "New Folder",
        "icon": "$(new-folder)"
      },
      {
        "command": "e2b.deleteItem",
        "title": "Delete",
        "icon": "$(trash)"
      },
      {
        "command": "e2b.renameItem",
        "title": "Rename",
        "icon": "$(edit)"
      },
      {
        "command": "e2b.searchFiles",
        "title": "E2B: Search Files",
        "icon": "$(search)"
      },
      {
        "command": "e2b.searchSandboxes",
        "title": "E2B: Search Sandboxes",
        "icon": "$(filter)"
      },
      {
        "command": "e2b.clearSandboxFilter",
        "title": "E2B: Clear Sandbox Filter",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "e2b.clearSandboxFilter",
          "when": "view == e2bSandboxes && e2b.sandboxListFiltered",
          "group": "navigation"
        },
        {
          "command": "e2b.searchSandboxes",
          "when": "view == e2bSandboxes",
          "group": "navigation"
        },
        {
          "command": "e2b.refresh",
          "when": "view == e2bSandboxes",
          "group": "navigation"
        },
        {
          "command": "e2b.refresh",
          "when": "view == e2bFiles && e2b.connected",
          "group": "navigation"
        },
        {
          "command": "e2b.disconnect",
          "when": "view == e2bFiles && e2b.connected",
          "group": "navigation"
        },
        {
          "command": "e2b.openTerminal",
          "when": "view == e2bFiles && e2b.connected",
          "group": "navigation"
        },
        {
          "command": "e2b.newFile",
          "when": "view == e2bFiles && e2b.connected",
          "group": "navigation"
        },
        {
          "command": "e2b.newFolder",
          "when": "view == e2bFiles && e2b.connected",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "e2b.connect",
          "when": "view == e2bSandboxes && viewItem == sandbox",
          "group": "inline"
        },
        {
          "command": "e2b.disconnect",
          "when": "view == e2bSandboxes && viewItem == sandboxConnected",
          "group": "inline"
        },
        {
          "command": "e2b.openFile",
          "when": "view == e2bFiles && viewItem == file",
          "group": "navigation"
        },
        {
          "command": "e2b.newFile",
          "when": "view == e2bFiles && viewItem == directory",
          "group": "1_new"
        },
        {
          "command": "e2b.newFolder",
          "when": "view == e2bFiles && viewItem == directory",
          "group": "1_new"
        },
        {
          "command": "e2b.renameItem",
          "when": "view == e2bFiles && viewItem =~ /file|directory/",
          "group": "2_edit"
        },
        {
          "command": "e2b.deleteItem",
          "when": "view == e2bFiles && viewItem =~ /file|directory/",
          "group": "2_edit"
        },
        {
          "command": "e2b.searchFiles",
          "when": "view == e2bFiles && viewItem == sandboxRoot",
          "group": "inline"
        },
        {
          "command": "e2b.searchFiles",
          "when": "view == e2bFiles && viewItem == directory",
          "group": "inline@1"
        },
        {
          "command": "e2b.searchFiles",
          "when": "view == e2bFiles && viewItem =~ /directory|sandboxRoot/",
          "group": "3_search"
        }
      ]
    },
    "configuration": {
      "title": "E2B Sandbox Explorer",
      "properties": {
        "e2b.apiKey": {
          "type": "string",
          "default": "",
          "description": "E2B API key. Falls back to E2B_API_KEY environment variable if not set.",
          "order": 1
        },
        "e2b.searchIgnorePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            ".local",
            ".npm",
            ".pnpm-store",
            "node_modules",
            ".next",
            ".nuxt",
            "dist",
            "out",
            ".cache",
            ".parcel-cache",
            ".turbo",
            "coverage",
            "__pycache__",
            ".venv",
            "venv",
            "env",
            ".pytest_cache",
            ".mypy_cache",
            ".tox",
            ".hypothesis",
            "*.egg-info",
            ".eggs",
            "target",
            "vendor",
            ".gradle",
            ".mvn",
            ".bundle",
            ".git",
            ".svn",
            ".hg",
            ".idea",
            ".vscode",
            ".vs",
            ".DS_Store",
            "Thumbs.db",
            "tmp",
            "temp",
            ".tmp",
            ".temp"
          ],
          "description": "File and directory patterns to ignore during file search (when not using git). These patterns are passed to the 'find' command with -prune.",
          "order": 2
        }
      }
    }
  },
  "scripts": {
    "compile": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "watch": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --watch",
    "package": "bun run compile && vsce package"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/vscode": "^1.108.1",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.2",
    "typescript": "^5"
  },
  "dependencies": {
    "@e2b/code-interpreter": "^2.3.3"
  }
}
